
public class Animal {

	private int age;
	private String species;
	private String noise;

	public Animal(int age, String species) {
		this.age = age;
		this.species = species;
	}

	public Animal(int age, String species, String noise) {
		this.age = age;
		this.species = species;
		this.noise = noise;

	}

	public int getage() {
		return age;
	}

	public void setName(int age) {
		this.age = age;
	}

}
