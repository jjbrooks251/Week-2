
public class Person {

	public String Firstname;
	public int age;
	public String Occupation;

	public Person(String word) {

		this.Firstname = word;

	}

	public Person(String first, int num, String job) {

		this.Firstname = first;
		this.age = num;
		this.Occupation = job;
	}

	public void Print() {

		System.out.println(Firstname + ", " + age + ", " + Occupation);

	}

}
