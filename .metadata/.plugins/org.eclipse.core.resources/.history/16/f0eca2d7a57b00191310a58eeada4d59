
public class q7 {

	public static void main(String[] args) {

		System.out.println(range(10));

	}

	public static int range(int value) {

		for (int i = 0; i < value; i++) {
			System.out.print(i + " ");
			System.out.println(mod(i));
		}

		return value;
	}

	public static boolean mod(int input) {

		return ((input % 5) == 0);
	}

}
