package Classes;

import java.util.Date;

public class Book extends General {

	private Boolean coverType;

	public Book(String location, int itemId, String publisher, double id, Date release, int stock, String coverCol,
			Boolean condition, int pageNo, Boolean coverType) {
		super(location, itemId, publisher, id, release, stock, coverCol, condition, pageNo);
		this.coverType = coverType;
	}

	public Boolean getCoverType() {
		return coverType;
	}

	public void setCoverType(Boolean coverType) {
		this.coverType = coverType;
	}

}
