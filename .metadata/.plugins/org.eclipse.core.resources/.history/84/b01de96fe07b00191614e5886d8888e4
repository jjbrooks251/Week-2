
public class Person {

	String Firstname = name() + ", ";
	
	String Occupation = ", " + Job(" ");

	public String name(String word) {

		return Firstname;
	}
	
	public String age(int num) {
		String age = Integer.toString(23);
		
		return age;
		
	}

	public String Job(String title) {

		return Occupation;
	}

	public String combo(String name, String job) {

		String s = "er";

		System.out.println(this.name(name) + age + this.Job(job));

		return s;

	}

}
